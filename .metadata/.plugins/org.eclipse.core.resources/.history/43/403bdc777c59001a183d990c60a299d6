package com.coachingProgram;

public class MissingNumber {

	public static void main(String[] args) {
		int[] x = { 1, 2, 3, 4, 5, 6, 7, 8, 10 };
		int sum = 0;
		for (int i = 0; i < x.length; i++) {
			sum = sum + x[i];
		}
		System.out.println(sum);
		
		int sum1 = 0;
		for (int j = 1; j <= 10; j++) {
			sum1 = sum1 + j;
		}
		System.out.println(sum1);
		
	}

}
